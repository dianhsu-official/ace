    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'ace'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=ace",
                    "--package=ace"
                ],
                "filter": {
                    "name": "ace",
                    "kind": "bin"
                }
            },
            "args": [
                "account"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}